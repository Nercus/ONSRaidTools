<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">

    <!--What should the parent name be? Do we have the parent as the main window as the parent?-->
    <Frame name="ONSSelectViewTemplate" virtual ="true" hidden="true" toplevel="true" enableMouse="true">
        <Size x="300" y="300"/>
        <Layers>
            <Layer level="ARTWORK">
                <Texture atlas="auctionhouse-background-buy-commodities">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" />
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" />
                    </Anchors>
                </Texture>
                <FontString name="$parentSelectView" inherits="GameFontNormal" justifyH="CENTER" text="Select View">
                    <Size x="300" y="0" />
                    <Anchors>
                        <Anchor point="CENTER" />
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <Button parentKey="backButton" frameLevel="800" text="BACK">
                <ButtonText name="Back" inherits="GameFontHighlight">
                </ButtonText>
                <Size x="140" y="30"/>
                <Anchors>
                    <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="6" y="6"/>
                </Anchors>
                <NormalTexture atlas="UI-CharacterCreate-LargeButton" alpha="0.6"/>
                <HighlightTexture atlas="UI-CharacterCreate-LargeButton" alpha="0.8"/>
                <PushedTexture atlas="UI-CharacterCreate-LargeButton" alpha="1"/>
            </Button>

            <Button parentKey="settingsButton" frameLevel="800" text="SETTINGS">
                <ButtonText name="Settings" inherits="GameFontHighlight">
                </ButtonText>
                <Size x="140" y="30"/>
                <Anchors>
                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="-6" y="6"/>
                </Anchors>
                <NormalTexture atlas="UI-CharacterCreate-LargeButton" alpha="0.6"/>
                <HighlightTexture atlas="UI-CharacterCreate-LargeButton" alpha="0.8"/>
                <PushedTexture atlas="UI-CharacterCreate-LargeButton" alpha="1"/>
            </Button>
        </Frames>
    </Frame>
</Ui>
