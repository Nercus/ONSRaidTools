<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">
    <Button name="ONSRaidToolsEncounterButtonTemplate" virtual="true" hidden="true" enableMouse="true">
        <Size x="145" y="30"/>
        <Layers>
            <Layer level="DIALOG">
                <Texture parentKey="icon">
                    <Size x="30" y="30"></Size>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" />
                        <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" />
                    </Anchors>
                </Texture>
                <FontString text="Encounter" parentKey="label" inherits="GameFontNormal">
                    <Anchors>
                        <Anchor point="LEFT" relativePoint="RIGHT" relativeKey="$parent.icon" />
                        <Anchor point="RIGHT" relativePoint="RIGHT" />
                    </Anchors>
                </FontString>
            </Layer>
            <Layer level="HIGHLIGHT">
                <Texture parentKey="highlight" atlas="Options_List_Active" alpha="0.5">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="13"/>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="13" y="-1" />
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
    </Button>
</Ui>
